@model IEnumerable<SereneSample.BasicSamples.PeriodicGrossSalesModel>

<table id="dt_detail" class="table table-striped table-bordered" cellspacing="0">
    <thead>
        <tr>
            <th>Contact Name</th>
            <th>Product Name</th>
            <th>Gross Sales</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.Encode(item.ContactName)</td>
                <td>@Html.Encode(item.ProductName)</td>
                <td align="right">@Html.Encode(item.GrossAmount)</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <th></th>
            <th></th>
            <th style="text-align:right"></th>
        </tr>
    </tfoot>
</table>








